<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tooltip?>

<VBox fx:id="window" prefHeight="500.0" prefWidth="360.0" styleClass="dev-fx-body-dialog" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="launch.Controller">
    <children>
        <AnchorPane prefHeight="30">
            <children>
                <Button fx:id="closeWindow" mnemonicParsing="false" styleClass="dev-fx-window-close" text="x" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <tooltip>
                        <Tooltip text="close" />
                    </tooltip>
                </Button>
            </children>
        </AnchorPane>
      <AnchorPane prefHeight="470.0" prefWidth="360.0">
         <children>
            <VBox prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="50.0">
               <children>
                  <Label alignment="CENTER" prefHeight="39.0" prefWidth="260.0" styleClass="dev-fx-title-login" text="员工账号注册" textAlignment="CENTER" textFill="#000000b0" VBox.vgrow="ALWAYS">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label contentDisplay="CENTER" styleClass="dev-fx-label-login" text="姓名" />
                        <TextField fx:id="nameTextField" alignment="CENTER" styleClass="dev-fx-textField-input" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label styleClass="dev-fx-label-login" text="密码" />
                        <PasswordField fx:id="passwordField" styleClass="dev-fx-textField-input" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label styleClass="dev-fx-label-login" text="确认密码" />
                        <PasswordField fx:id="confirmPasswordField" styleClass="dev-fx-textField-input" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label styleClass="dev-fx-label-login" text="员工类型" />
                        <ChoiceBox fx:id="typeChoiceBox" prefWidth="150.0" styleClass="dev-fx-choiceBox-login" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" layoutX="10.0" layoutY="368.0" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="applyButton" mnemonicParsing="false" styleClass="dev-fx-button_login" text="确认" />
                     </children></HBox>
               </children>
            </VBox>
         </children>
      </AnchorPane>
    </children>
</VBox>
